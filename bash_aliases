alias update='sudo apt-get update'
alias upgrade='sudo apt-get upgrade'
alias upgr='sudo apt-get update && sudo apt-get upgrade'
alias srch='sudo apt-cache search'
alias inst='sudo apt-get install'

alias tube='youtube-dl -o "%(title)s.%(ext)s"'

alias csk='cd $HOME/src/kde/src && cd'
alias cbk='cd $HOME/src/kde/build && cd'
alias cs='cd $HOME/src/ && cd'
alias dn='cd $HOME/downloads'
alias cdchef='cd $HOME/codes/practice/codechef'
alias cdtmp='cd $HOME/temp'

alias rm='rm -vi'
alias mv='mv -vi'
alias cp='cp -vi'

alias hist='history | grep'

alias mkdir='mkdir -pv'

alias wget='wget -c'

alias df='df -Tha --total'
alias free='free -mt'

alias cd..='cd ..'
alias ..='cd ..'

alias sl='ls'

alias lsl='ls -lhFA | less'

alias cputemp='sensors | grep Core'
